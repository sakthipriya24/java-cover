class Main{  
int id;  
String name;  
//method to display the value of id and name  
void display(){System.out.println(id+" "+name);}  
  
public static void main(String args[]){  
//creating objects  
Main s1=new Main();  
Main s2=new Main();  
//displaying values of the object  
s1.display();  //prints 0 and null
s2.display();  
}  
} 
