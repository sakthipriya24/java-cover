class Student{  
 int rollno;  
 String name;  
 void insert(int r, String n){  
  rollno=r;  
  name=n;  
 }  
 void display(){System.out.println(rollno+" "+name);}  
}  
class Main{  
 public static void main(String args[]){  
  Student s1=new Student();  
  Student s2=new Student();  
  s1.insert(1,"sakthi");  
  s2.insert(2,"priya");  
  s1.display();  
  s2.display();  
 }  
}  
